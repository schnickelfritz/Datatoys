<div class="overflow-auto {{ small == true ? 'd-inline-block' : '' }}">
    <table class="table{{ small == true ? ' table-sm' : '' }} table-dark{{ stripes == true ? ' table-striped' : '' }} table-hover">
        <thead>
        <tr>
            {% for th in headers %}
                <th scope="col">
                    {% if th != '' %}
                        {{- th|trans -}}
                    {% endif %}
                </th>
            {% endfor %}
        </tr>
        </thead>
        <tbody class="table-group-divider">
        {% block content %}{% endblock %}
        </tbody>
    </table>
</div>
